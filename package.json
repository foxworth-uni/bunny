{
  "name": "bunny-workspace",
  "version": "0.1.0",
  "private": true,
  "description": "Bunny - Fast React docs with MDX v3",
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "benchmark": "turbo run benchmark",
    "benchmark:simple": "turbo run benchmark:simple --filter=@bunny/benchmarks",
    "benchmark:complex": "turbo run benchmark:complex --filter=@bunny/benchmarks",
    "benchmark:large": "turbo run benchmark:large --filter=@bunny/benchmarks",
    "benchmark:realworld": "turbo run benchmark:realworld --filter=@bunny/benchmarks",
    "clean": "turbo run clean"
  },
  "keywords": [
    "mdx",
    "markdown",
    "react",
    "documentation",
    "rust"
  ],
  "author": "Bunny Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nine-gen/bunny.git"
  },
  "homepage": "https://bunny.dev",
  "devDependencies": {
    "turbo": "^2.6.0"
  }
}

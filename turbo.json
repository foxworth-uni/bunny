{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "outputLogs": "new-only"
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputLogs": "full"
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputLogs": "new-only"
    },
    "test": {
      "dependsOn": ["^build"],
      "outputLogs": "new-only"
    },
    "benchmark": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputLogs": "full"
    },
    "benchmark:simple": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputLogs": "full"
    },
    "benchmark:complex": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputLogs": "full"
    },
    "benchmark:large": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputLogs": "full"
    },
    "benchmark:realworld": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputLogs": "full"
    },
    "clean": {
      "cache": false
    }
  }
}

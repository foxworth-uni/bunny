{
  "name": "bunny-next",
  "version": "0.1.0",
  "description": "Fast Next.js MDX integration powered by Rust",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./rsc": {
      "types": "./dist/rsc.d.ts",
      "import": "./dist/rsc.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && node scripts/copy-native-bindings.js",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bunny/mdx-node": "workspace:*",
    "@bunny/mdx-runtime": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0"
  },
  "keywords": [
    "mdx",
    "next",
    "nextjs",
    "markdown",
    "react",
    "rust",
    "fast"
  ],
  "author": "Bunny MDX",
  "license": "MIT"
}

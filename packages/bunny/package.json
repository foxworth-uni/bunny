{
  "name": "bunny",
  "version": "0.1.0",
  "description": "Fast MDX v3 compiler for the browser - compile MDX to JSX with WebAssembly",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./wasm": {
      "types": "./dist/wasm.d.ts",
      "import": "./dist/bunny_wasm.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:wasm && pnpm build:js",
    "build:wasm": "wasm-pack build --target web --release ../../crates/bunny-wasm --out-dir ../../packages/bunny/dist",
    "build:js": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mdx",
    "markdown",
    "compiler",
    "wasm",
    "webassembly",
    "react",
    "browser"
  ],
  "author": "Bunny Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nine-gen/bunny"
  },
  "homepage": "https://bunny.dev",
  "devDependencies": {
    "@vitest/ui": "^2.1.9",
    "happy-dom": "^15.11.7",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  }
}

{
  "name": "@bunny/mdx-wasm",
  "version": "0.1.0",
  "description": "WASM bindings for bunny-mdx compiler - optimized for bundlers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./wasm": {
      "types": "./wasm/bunny_wasm.d.ts",
      "import": "./wasm/bunny_wasm.js"
    },
    "./wasm.wasm": {
      "import": "./wasm/bunny_wasm_bg.wasm"
    }
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "pnpm build:wasm && pnpm build:js",
    "build:wasm": "wasm-pack build --target bundler --release ../../crates/bunny-wasm --out-dir ../../packages/mdx-wasm/wasm",
    "build:js": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mdx",
    "markdown",
    "compiler",
    "wasm",
    "webassembly",
    "bundler"
  ],
  "author": "Bunny Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nine-gen/bunny"
  },
  "devDependencies": {
    "typescript": "^5.9.0"
  }
}

